
public class Prac3 {

	public static void main(String[] args) {
		
		//most common primitive type
		
		//integer categories
		boolean isGood = true;
		byte b = 127; //1111111 limited in scope
		byte d = 'c';
		char ch = 'c'; //int -> char is ASCII, better is to use UNICODE
		short s = 3204;
		int val = 43566; //4Bytes
		long new_val = 222345555L; //8B
				
		// floating point categories
		float tempC = 32.74f;
		double tempR = 43.98;
		double divD = 1.0/3.0;
		float divF = 1.0f/3.0f;
		System.out.println(divF);
		System.out.println(divD);
		


	}
	 

}
