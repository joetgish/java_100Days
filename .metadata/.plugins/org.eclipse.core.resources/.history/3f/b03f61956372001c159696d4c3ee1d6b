
public class Prac3 {

	public static void main(String[] args) {
		
		//most common primitive type
		
		//integer categories
		boolean isGood = true;
		byte b = 127; //1111111 limited in scope
		byte d = 'c';
		char ch = 'c'; //int -> char is ASCII, better is to use UNICODE
		short s = 3204;
		int val = 43566; //4Bytes
		long val = 222345555; //8B
				
		// floating point categories
		


	}
	 

}
